*
  margin 0
  padding 0
  border 0
  outline 0
  box-sizing border-box
  line-height 1

ol,
ul,
li
  list-style none

a
  text-decoration none
  outline none
  display block

i
  font-style normal

button
  background transparent
  outline none

table
  border-collapse collapse
  border-spacing 0

img
  display block
  vertical-align top

input::-webkit-input-placeholder /* WebKit browsers */
  color #C7C7C7
input::-moz-placeholder /* Mozilla Firefox 19+ */
  color #C7C7C7
input:-moz-placeholder /* Mozilla Firefox 4 to 18 */
  color #C7C7C7
input:-ms-input-placeholder /* Internet Explorer 10-11 */
  color #C7C7C7

input-webkit-input-placeholder
  /* WebKit browsers */
  color #A1A1A1

input-moz-placeholder
  /* Mozilla Firefox 4 to 18 */
  color #A1A1A1

input-moz-placeholder
  /* Mozilla Firefox 19+ */
  color #A1A1A1

input-ms-input-placeholder
  /* Internet Explorer 10+ */
  color #A1A1A1

.hide
  display none !important

.w
  width 100%

.wrap
  box-sizing border-box
  min-height 100%
  height auto !important
  height 100%
  position relative

.mgauto
  margin-left auto
  margin-right auto

html,
body
  word-break break-all
  -webkit-user-select none
  -webkit-text-size-adjust none
  width 100%
  height 100%
  overflow hidden

body
  background rgba(239, 238, 244, 1)
  font-family Helvetica, Tahoma, Arial, "PingFangSC-Regular", "Hiragino Sans GB", "Heiti SC", "Microsoft YaHei", "WenQuanYi Micro Hei"


/**
 * 滚动条样式
 */

-webkit-scrollbar
  width 0
  display none

.ali_icon
  width 100%
  height 100%

.full_box
  width 100%
  height 100%
  overflow scroll
  -webkit-overflow-scrolling touch

